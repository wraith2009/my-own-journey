#include <bits/stdc++.h>
using namespace std;

int main() {
int m,n;
cin>>m;
cin>>n;
int arr[n];
int que[m];
for(int i=0;i<n;i++){
    cin>>arr[i];
}
for(int i=0;i<m;i++){
    cin>>que[i];
}
   
   for(int i=0;i<m;i++){
       int j=0;
       while(j<n && que[i]>=arr[j]){
           j++;
       }
       cout<<j<<endl;
   } 

    return 0;
}
